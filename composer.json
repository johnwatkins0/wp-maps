{
  "name": "colbycomms/wp-maps",
  "description": "Display Google Maps from data entered via WordPress.",
  "type": "library",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "wp-coding-standards/wpcs": "^0.14.0"
  },
  "authors": [
    {
      "name": "John Watkins",
      "email": "john.watkins@colby.edu"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "htmlburger/carbon-fields": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "ColbyComms\\Maps\\": "maps/"
    },
    "files": ["maps/index.php"]
  },
  "scripts": {
    "phpcbf": "phpcbf --extensions=php",
    "lint": "phpcs --extensions=php -s",
    "test": "composer lint"
  }
}
